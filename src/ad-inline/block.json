{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "www/ad-inline",
	"version": "0.1.0",
	"title": "Inline ad",
	"category": "ads",
	"icon": "money",
	"description": "Two column add with an image for inside a blog post.",
	"example": {},
	"textdomain": "www",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./editor.css",
	"style": "file:./style.css",
	"attributes": {
		"instanceId": {
			"type": "string",
			"default": ""
		},
		"heading": {
			"type": "object",
			"attribute": "html",
			"default": {
				"text": "Try Mindspun today!"
			}
		},
		"tagline": {
			"type": "string",
			"attribute": "html",
			"default": {
				"text": "No credit card required"
			}
		},
		"button": {
			"type": "object",
			"default": {
				"text": "Learn More",
				"link": "https://www.mindspun.com",
				"align": "none"
			}
		},
		"buttonHover": {
			"type": "object",
			"default": {}
		},
		"container": {
			"type": "object",
			"default": {}
		},
		"content": {
			"type": "object",
			"default": {}
		},
		"icon": {
			"type": "object",
			"default": {
				"url": "",
				"id": "",
				"alt": "Mindspun Icon"
			}
		},
		"media": {
			"type": "object",
			"default": {
				"url": "",
				"id": "",
				"alt": "Mindspun Image"
			}
		},
		"variation": {
			"enum": ["default", "left"],
			"default": "default"
		}
	}
}
