{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "www/ad-responsive",
	"version": "0.1.0",
	"title": "Responsive Ad",
	"category": "ads",
	"description": "Fully responsive inline ad.",
	"example": {},
	"textdomain": "www",
	"editorScript": ["wpx","file:./index.js"],
	"editorStyle": ["wpx","file:./editor.css"],
	"style": "file:./style.css",
	"attributes": {
		"blockId": {
			"type": "string",
			"default": ""
		},
		"heading": {
			"type": "object",
			"attribute": "html",
			"default": {
				"text": "Try Mindspun today!"
			},
			"selector": ".ad-heading"
		},
		"tagline": {
			"type": "object",
			"attribute": "html",
			"default": {
				"text": "No credit card required"
			},
			"selector": ".ad-tagline"
		},
		"button": {
			"type": "object",
			"default": {
				"text": "Learn More",
				"link": "https://www.mindspun.com"
			},
			"selector": ".ad-btn"
		},
		"container": {
			"type": "object",
			"default": {},
			"selector": ".ad-container"
		},
		"content": {
			"type": "object",
			"default": {},
			"selector": ".ad-content"
		},
		"icon": {
			"type": "object",
			"default": {
				"url": "",
				"id": "",
				"alt": "Mindspun Icon"
			},
			"selector": ".ad-icon"
		},
		"media": {
			"type": "object",
			"default": {
				"url": "",
				"id": "",
				"alt": "Mindspun Image"
			},
			"selector": ".ad-img"
		},
		"variation": {
			"enum": ["default", "left"],
			"default": "default"
		}
	}
}
