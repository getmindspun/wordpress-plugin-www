{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "www/ad-basic",
	"version": "0.1.0",
	"title": "Basic ad",
	"category": "ads",
	"icon": "flag",
	"description": "Basic inline add with a CTA",
	"textdomain": "www",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./editor.css",
	"style": "file:./style.css",
	"attributes": {
		"instanceId": {
			"type": "string",
			"default": ""
		},
		"heading": {
			"type": "object",
			"attribute": "html",
			"default": {
				"text": "Try Mindspun today!"
			}
		},
		"tagline": {
			"type": "string",
			"attribute": "html",
			"default": {
				"text": "No credit card required"
			}
		},
		"button": {
			"type": "object",
			"default": {
				"text": "Learn More",
				"link": "https://www.mindspun.com"
			}
		},
		"buttonHover": {
			"type": "object",
			"default": {}
		},
		"container": {
			"type": "object",
			"default": {}
		}
	}
}
