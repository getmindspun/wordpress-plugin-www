{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "www/ad-basic",
	"version": "0.1.0",
	"title": "Basic ad",
	"category": "ads",
	"icon": "flag",
	"description": "Basic inline add with a CTA",
	"textdomain": "www",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./editor.css",
	"style": "file:./style.css",
	"attributes": {
		"h2": {
			"type": "string",
			"source": "attribute",
			"selector": "h2",
			"attribute": "text",
			"default": "Try Mindspun today!"
		},
		"h3": {
			"type": "string",
			"source": "attribute",
			"selector": "h3",
			"attribute": "text",
			"default": "no signup required"
		},
		"button": {
			"type": "object",
			"default": {
				"text": "Learn More",
				"link": "https://www.mindspun.com"
			}
		}
	}
}
